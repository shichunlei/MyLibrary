<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmoothCheckBox">
        <attr format="integer" name="duration" />
        <attr format="dimension" name="stroke_width" />
        <attr format="color" name="color_tick" />
        <attr format="color" name="color_checked" />
        <attr format="color" name="color_unchecked" />
        <attr format="color" name="color_unchecked_stroke" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <!-- 描边宽度 -->
        <attr format="dimension" name="border_width" />
        <!-- 描边颜色 -->
        <attr format="color" name="border_color" />
        <!--圆角大小-->
        <attr format="dimension" name="corner_radius" />
        <!--左上圆角大小-->
        <attr format="dimension" name="leftTop_corner_radius" />
        <!--右上圆角大小-->
        <attr format="dimension" name="rightTop_corner_radius" />
        <!--左下圆角大小-->
        <attr format="dimension" name="leftBottom_corner_radius" />
        <!--右下圆角大小-->
        <attr format="dimension" name="rightBottom_corner_radius" />
        <!--图片类型：圆角或者圆形-->
        <attr format="enum" name="type">
            <enum name="oval" value="2" />
            <enum name="round" value="1" />
            <enum name="circle" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundTextView">
        <!--四周圆角度、左上、右上、左下、右下-->
        <attr format="dimension" name="corner_radius_all" />
        <attr format="dimension" name="corner_radius_top_left" />
        <attr format="dimension" name="corner_radius_top_right" />
        <attr format="dimension" name="corner_radius_bottom_left" />
        <attr format="dimension" name="corner_radius_bottom_right" />

        <!--虚线边框宽度/间隔-->
        <attr format="dimension" name="border_dash_width" />
        <attr format="dimension" name="border_dash_gap" />

        <!--边框宽度-->
        <attr format="dimension" name="border_width_normal" />
        <attr format="dimension" name="border_width_pressed" />
        <attr format="dimension" name="border_width_unable" />

        <!--边框颜色-->
        <attr format="color" name="border_color_normal" />
        <attr format="color" name="border_color_pressed" />
        <attr format="color" name="border_color_unable" />

        <!--背景颜色-->
        <attr format="color" name="background_normal" />
        <attr format="color" name="background_pressed" />
        <attr format="color" name="background_unable" />

        <!--文本颜色-->
        <attr format="color" name="text_color_normal" />
        <attr format="color" name="text_color_pressed" />
        <attr format="color" name="text_color_unable" />

        <!--图标资源-->
        <attr format="reference" name="icon_src_normal" />
        <attr format="reference" name="icon_src_pressed" />
        <attr format="reference" name="icon_src_unable" />

        <!--图标、大小、位置-->
        <attr format="dimension" name="icon_height" />
        <attr format="dimension" name="icon_width" />
        <attr name="icon_direction">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ExpandIconView">
        <attr format="boolean" name="eiv_roundedCorners" />
        <attr format="boolean" name="eiv_switchColor" />
        <attr format="color" name="eiv_color" />
        <attr format="color" name="eiv_colorMore" />
        <attr format="color" name="eiv_colorLess" />
        <attr format="integer" name="eiv_animationDuration" />
        <attr format="dimension" name="eiv_padding" />
    </declare-styleable>

    <!-- 可翻转View -->
    <declare-styleable name="FlipView">
        <attr format="boolean" name="flip_touch" />
        <attr format="integer" name="flip_duration" />
        <attr format="boolean" name="flip_enabled" />
        <attr format="enum" name="flip_state">
            <enum name="front" value="0" />
            <enum name="back" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NumberAnimTextView">
        <attr format="boolean" name="enable_anim" />
        <attr format="string" name="start_number" />
        <attr format="string" name="end_number" />
        <attr format="integer" name="number_duration" />
        <attr format="string" name="postfix" />
        <attr format="string" name="prefix" />
    </declare-styleable>

    <!-- 角标 -->
    <declare-styleable name="LabelView">
        <!-- 设置文字内容 -->
        <attr format="string" name="lv_text" />
        <!-- 设置文字颜色,默认#ffffff -->
        <attr format="color" name="lv_text_color" />
        <!-- 设置文字大小,默认11sp -->
        <attr format="dimension" name="lv_text_size" />
        <!-- 设置文字是否支持加粗,默认true -->
        <attr format="boolean" name="lv_text_bold" />
        <!-- 设置文字是否支持全部大写,默认true-->
        <attr format="boolean" name="lv_text_all_caps" />
        <!-- 设置背景颜色,默认"#FF4081"-->
        <attr format="color" name="lv_background_color" />
        <!-- 设置LabelView所在矩形最小宽高,默认mFillTriangle?35dp:50dp-->
        <attr format="dimension" name="lv_min_size" />
        <!-- 设置文字上下padding,默认3.5dp,mFillTriangle为true时无效-->
        <attr format="dimension" name="lv_padding" />
        <!-- 设置LabelView方向 -->
        <attr format="enum" name="lv_gravity">
            <enum name="TOP_LEFT" value="51" />
            <enum name="TOP_RIGHT" value="53" />
            <enum name="BOTTOM_LEFT" value="83" />
            <enum name="BOTTOM_RIGHT" value="85" />
        </attr>
        <!-- 设置是否填充三角区域,默认false -->
        <attr format="boolean" name="lv_fill_triangle" />
    </declare-styleable>

    <!-- 可展开(Expand)的TextView -->
    <declare-styleable name="ExpandTextView">
        <!--设置开始折叠的行数-->
        <attr format="integer" name="maxCollapsedLines" />
        <!--展开/折叠动画时长-->
        <attr format="integer" name="animDuration" />
        <!--透明度渐变起始值-->
        <attr format="float" name="animAlphaStart" />
        <!--指示展开状态的图标-->
        <attr format="reference" name="expandDrawable" />
        <!--指示折叠状态的图标-->
        <attr format="reference" name="collapseDrawable" />
        <!--指示状态的图标的对齐方式,有左/上,居中,右/下可选-->
        <attr format="enum" name="arrowAlign">
            <enum name="bottom" value="0" />
            <enum name="top" value="1" />
            <enum name="center" value="2" />
            <enum name="right" value="0" />
            <enum name="left" value="1" />
        </attr>
        <!--指示状态的图标位置,两种可选,在text的右边和在text的下面-->
        <attr format="enum" name="arrowPosition">
            <enum name="right" value="0" />
            <enum name="below" value="1" />
        </attr>
        <!--可以设置图标离文字的间距-->
        <attr format="dimension" name="arrowPadding" />
    </declare-styleable>

    <!-- 水波 -->
    <declare-styleable name="WaveLoadingView">
        <!-- 描边宽度, default is 0 -->
        <attr format="dimension" name="wlv_borderWidth" />
        <!-- 描边颜色 -->
        <attr format="color" name="wlv_borderColor" />
        <!-- 进度值, default is 50 -->
        <attr format="integer" name="wlv_progressValue" />
        <!-- 形状, default is circle -->
        <attr format="enum" name="wlv_shapeType">
            <enum name="triangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="square" value="2" />
            <enum name="rectangle" value="3" />
        </attr>
        <!-- Triangle direction, default is north -->
        <attr format="enum" name="wlv_triangle_direction">
            <enum name="north" value="0" />
            <enum name="south" value="1" />
            <enum name="east" value="2" />
            <enum name="west" value="3" />
        </attr>
        <!-- Is round rectangle, default is false -->
        <attr format="boolean" name="wlv_round_rectangle" />
        <!-- Round Rectangle corners, default is 30 -->
        <attr format="integer" name="wlv_round_rectangle_x_and_y" />
        <!-- Wave color -->
        <attr format="color" name="wlv_waveColor" />
        <!-- Wave background color -->
        <attr format="color" name="wlv_wave_background_Color" />
        <!-- Wave amplitude -->
        <attr format="float" name="wlv_waveAmplitude" />
        <!-- Top title content, default is null -->
        <attr format="string" name="wlv_titleTop" />
        <!-- Center title content, default is null -->
        <attr format="string" name="wlv_titleCenter" />
        <!-- Bottom title content, default is null -->
        <attr format="string" name="wlv_titleBottom" />
        <!-- Top title size, default is 18 -->
        <attr format="dimension" name="wlv_titleTopSize" />
        <!-- Center title size, default is 22 -->
        <attr format="dimension" name="wlv_titleCenterSize" />
        <!-- Bottom size, default is 18 -->
        <attr format="dimension" name="wlv_titleBottomSize" />
        <!-- Top title color -->
        <attr format="color" name="wlv_titleTopColor" />
        <!-- Center title color -->
        <attr format="color" name="wlv_titleCenterColor" />
        <!-- Bottom title color -->
        <attr format="color" name="wlv_titleBottomColor" />
        <!-- Top title stroke color -->
        <attr format="color" name="wlv_titleTopStrokeColor" />
        <!-- Center title stroke color -->
        <attr format="dimension" name="wlv_titleTopStrokeWidth" />
        <!-- Bottom title stroke color -->
        <attr format="color" name="wlv_titleBottomStrokeColor" />
        <!-- Top title stroke width -->
        <attr format="dimension" name="wlv_titleBottomStrokeWidth" />
        <!-- Center title stroke width -->
        <attr format="color" name="wlv_titleCenterStrokeColor" />
        <!-- Bottom title stroke width -->
        <attr format="dimension" name="wlv_titleCenterStrokeWidth" />
    </declare-styleable>

    <!-- 雷达图 -->
    <declare-styleable name="RadarView">
        <attr format="integer" name="radar_layer" />
        <attr format="boolean" name="rotation_enable" />
        <attr format="enum" name="web_mode">
            <enum name="polygon" value="1" />
            <enum name="circle" value="2" />
        </attr>
        <attr format="float" name="max_value" />
        <attr format="color" name="radar_line_color" />
        <attr format="boolean" name="radar_line_enable" />
        <attr format="dimension" name="radar_line_width" />
        <attr format="color" name="vertex_text_color" />
        <attr format="dimension" name="vertex_text_size" />
        <attr format="dimension" name="vertex_text_offset" />
    </declare-styleable>

    <!-- 数字进度条 -->
    <declare-styleable name="NumberProgressBar">
        <attr format="integer" name="progress_current" />
        <attr format="integer" name="progress_max" />

        <attr format="color" name="progress_unreached_color" />
        <attr format="color" name="progress_reached_color" />

        <attr format="dimension" name="progress_reached_bar_height" />
        <attr format="dimension" name="progress_unreached_bar_height" />

        <attr format="dimension" name="progress_text_size" />
        <attr format="color" name="progress_text_color" />

        <attr format="dimension" name="progress_text_offset" />

        <attr format="enum" name="progress_text_visibility">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="Themes">
        <attr format="reference" name="numberProgressBarStyle" />
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <!--最大值-->
        <attr format="float" name="max" />
        <!--最小值-->
        <attr format="float" name="min" />
        <!--两个按钮的最小间距-->
        <attr format="float" name="reserve" />
        <!--cells 等于0为普通模式，大于1时切换为刻度模式-->
        <attr format="integer" name="cells" />
        <!--是否关闭进度提示-->
        <attr format="boolean" name="hideProgressHint" />
        <!--拖动后的Seekbar颜色-->
        <attr format="color" name="lineColorSelected" />
        <!--默认的Seekbar颜色-->
        <attr format="color" name="lineColorEdge" />
        <!--进度为最小值或最大值时按钮的颜色，默认此属性不调用-->
        <attr format="color" name="thumbPrimaryColor" />
        <!--进度不为最小值或最大值时按钮的颜色，默认此属性不调用-->
        <attr format="color" name="thumbSecondaryColor" />
        <!--刻度文字，不设置的时候默认隐藏-->
        <attr format="reference" name="markTextArray" />
        <!--按钮的背景资源，不设置的时候默认为圆形按钮-->
        <attr format="reference" name="seekBarResId" />
        <!--进度提示背景资源，必须使用 9 path文件-->
        <attr format="reference" name="progressHintResId" />
        <!--刻度文字与进度条之间的距离-->
        <attr format="dimension" name="textPadding" />
        <!--刻度文字和进度提示文字的大小-->
        <attr format="dimension" name="textSize" />
        <!--进度提示背景的高度，不设置时根据文字尺寸自适应-->
        <attr format="dimension" name="hintBGHeight" />
        <!--进度提示背景的宽度，不设置时根据文字尺寸自适应-->
        <attr format="dimension" name="hintBGWith" />
        <!--进度提示背景和进度条之间的距离-->
        <attr format="dimension" name="hintBGPadding" />
        <!--进度条的高度-->
        <attr format="dimension" name="seekBarHeight" />
        <!--按钮的尺寸-->
        <attr format="dimension" name="thumbSize" />
        <!--刻度模式
            number 根据刻度的实际所占比例分配位置（markTextArray中必须都为数字）
            other 平分当前布局（markTextArray可以是任何字符）
        -->
        <attr format="enum" name="cellMode">
            <enum name="number" value="0" />
            <enum name="other" value="1" />
        </attr>
        <!--单向、双向模式 single 单向模式，只有一个按钮 range 双向模式，有两个按钮 -->
        <attr format="enum" name="seekBarMode">
            <enum name="single" value="1" />
            <enum name="range" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SwitchButton">
        <!-- 阴影半径 -->
        <attr format="reference|dimension" name="sb_shadow_radius" />
        <!-- 阴影偏移 -->
        <attr format="reference|dimension" name="sb_shadow_offset" />
        <!-- 阴影颜色 -->
        <attr format="reference|color" name="sb_shadow_color" />
        <!-- 关闭颜色 -->
        <attr format="reference|color" name="sb_uncheck_color" />
        <!-- 开启颜色 -->
        <attr format="reference|color" name="sb_checked_color" />

        <!-- 边框宽度 -->
        <attr format="reference|dimension" name="sb_border_width" />
        <!-- 开启指示器颜色 -->
        <attr format="reference|color" name="sb_checkline_color" />
        <!-- 开启指示器线宽 -->
        <attr format="reference|dimension" name="sb_checkline_width" />
        <!-- 关闭指示器颜色 -->
        <attr format="reference|color" name="sb_uncheckcircle_color" />
        <!-- 关闭指示器线宽 -->
        <attr format="reference|dimension" name="sb_uncheckcircle_width" />
        <!-- 关闭指示器半径 -->
        <attr format="reference|dimension" name="sb_uncheckcircle_radius" />

        <!-- 是否选中 -->
        <attr format="reference|boolean" name="sb_checked" />
        <!-- 是否启用阴影 -->
        <attr format="reference|boolean" name="sb_shadow_effect" />
        <!-- 动画时间，默认300ms -->
        <attr format="reference|integer" name="sb_effect_duration" />
        <!-- 按钮颜色 -->
        <attr format="reference|color" name="sb_button_color" />
        <!-- 是否显示指示器，默认true：显示 -->
        <attr format="reference|boolean" name="sb_show_indicator" />
        <!-- 背景色，默认白色 -->
        <attr format="reference|color" name="sb_background" />
        <!-- 是否启用特效，默认true -->
        <attr format="reference|boolean" name="sb_enable_effect" />
    </declare-styleable>

    <declare-styleable name="ThreeStateSwitch">
        <attr format="color" name="background_selected_color" />
        <attr format="color" name="background_normal_color" />
        <attr format="color" name="text_normal_color" />
        <attr format="color" name="text_selected_color" />
        <attr format="string" name="text_left" />
        <attr format="string" name="text_right" />
        <attr format="dimension" name="text_normal_size" />
        <attr format="dimension" name="text_selected_size" />
    </declare-styleable>

    <declare-styleable name="StateView">
        <attr format="reference" name="emptyResource" />
        <attr format="reference" name="retryResource" />
    </declare-styleable>

    <declare-styleable name="RulerView">
        <attr format="dimension" name="rulerHeight" />               <!--尺子的高度-->
        <attr format="dimension" name="rulerToResultgap" />          <!--尺子距离结果的高度-->
        <attr format="dimension" name="scaleGap" />                  <!--刻度间距-->
        <attr format="integer" name="scaleCount" />                  <!--刻度数-->
        <attr format="float" name="firstScale" />                    <!--默认选中的刻度-->
        <attr format="integer" name="maxScale" />                    <!--最大刻度-->
        <attr format="integer" name="minScale" />                    <!--最小刻度-->
        <attr format="color" name="bgColor" />                       <!--背景色-->
        <attr format="color" name="smallScaleColor" />               <!--小刻度的颜色-->
        <attr format="color" name="midScaleColor" />                 <!--中刻度的颜色-->
        <attr format="color" name="largeScaleColor" />               <!--大刻度的颜色-->
        <attr format="color" name="scaleNumColor" />                 <!--刻度数的颜色-->
        <attr format="color" name="resultNumColor" />                <!--结果字体的颜色-->
        <attr format="string" name="unit" />                         <!--单位-->
        <attr format="color" name="unitColor" />                     <!--单位颜色-->
        <attr format="dimension" name="smallScaleStroke" />          <!--小刻度的宽度-->
        <attr format="dimension" name="midScaleStroke" />            <!--中刻度的宽度-->
        <attr format="dimension" name="largeScaleStroke" />          <!--大刻度的宽度-->
        <attr format="dimension" name="resultNumTextSize" />         <!--结果字体大小-->
        <attr format="dimension" name="scaleNumTextSize" />          <!--刻度字体大小-->
        <attr format="dimension" name="unitTextSize" />              <!--单位字体大小-->
        <attr format="boolean" name="showScaleResult" />             <!--是否显示结果值-->
        <attr format="boolean" name="isBgRoundRect" />               <!--背景是否圆角-->
    </declare-styleable>

    <declare-styleable name="DecimalRulerView">
        <attr format="float" name="rv_defaultValue" />              <!-- 默认值 -->
        <attr format="float" name="rv_minValue" />                  <!-- 最小值 -->
        <attr format="float" name="rv_maxValue" />                  <!-- 最大值 -->
        <attr format="float" name="rv_spanValue" />                 <!-- 精度，最小支持0.1 -->
        <attr format="dimension" name="rv_itemSpacing" />           <!-- 每个刻度间的宽度 -->
        <attr format="dimension" name="rv_minLineHeight" />         <!-- 最短刻度线长度 -->
        <attr format="dimension" name="rv_maxLineHeight" />         <!-- 最长刻度线长度 -->
        <attr format="dimension" name="rv_middleLineHeight" />      <!-- 中间刻度线长度 -->
        <attr format="dimension" name="rv_minLineWidth" />          <!-- 最短刻度线宽度 -->
        <attr format="dimension" name="rv_maxLineWidth" />          <!-- 最长刻度线宽度 -->
        <attr format="dimension" name="rv_middleLineWidth" />       <!-- 中间刻度线宽度 -->
        <attr format="color" name="rv_scaleTextColor" />            <!-- 刻度盘文字颜色 -->
        <attr format="color" name="rv_minLineColor" />              <!-- 最短刻度线颜色 -->
        <attr format="color" name="rv_maxLineColor" />              <!-- 最大刻度线颜色 -->
        <attr format="color" name="rv_middleLineColor" />           <!-- 中间刻度线颜色 -->
        <attr format="dimension" name="rv_scaleTextSize" />         <!-- 刻度盘文字大小 -->
        <attr format="dimension" name="rv_textMarginTop" />         <!-- 刻度盘文字距离刻度边缘距离 -->
        <attr format="color" name="rv_indcatorColor" />             <!-- 指示器颜色 -->
        <attr format="dimension" name="rv_indcatorWidth" />         <!-- 指示器宽度，形状为三角时不起作用 -->
        <attr format="dimension" name="rv_indcatorHeight" />        <!-- 指示器高度，形状为三角时不起作用 -->
        <attr format="enum" name="rv_indcatorType">                 <!-- 指示器形状 -->
            <enum name="line" value="1" />                          <!-- 线 -->
            <enum name="triangle" value="2" />                      <!-- 三角 -->
        </attr>
        <attr format="color" name="rv_resultTextColor" />           <!-- 结果文字颜色 -->
        <attr format="color" name="rv_unitTextColor" />             <!-- 单位文字颜色 -->
        <attr format="dimension" name="rv_resultTextSize" />        <!-- 结果文字大小 -->
        <attr format="dimension" name="rv_unitTextSize" />          <!-- 单位文字大小 -->
        <attr format="string" name="rv_unit" />                     <!-- 单位 -->
        <attr format="boolean" name="rv_showResult" />              <!-- 是否显示结果 -->
        <attr format="boolean" name="rv_showUnit" />                <!-- 是否显示单位 -->
        <attr format="boolean" name="rv_alphaEnable" />             <!-- 是否刻度渐变 -->
    </declare-styleable>

    <declare-styleable name="CircleMenuView">
        <!-- Array of buttons icons. -->
        <attr format="reference" name="button_icons" />
        <!-- Array of buttons colors. -->
        <attr format="reference" name="button_colors" />
        <!-- Menu default icon. -->
        <attr format="reference" name="icon_menu" />
        <!-- Menu closed icon. -->
        <attr format="reference" name="icon_close" />
        <!-- Menu icon color. -->
        <attr format="color" name="icon_color" />
        <!-- Ring effect duration. -->
        <attr format="integer" name="duration_ring" />
        <!-- Menu opening animation duration. -->
        <attr format="integer" name="duration_open" />
        <!-- Menu closing animation duration. -->
        <attr format="integer" name="duration_close" />
        <!-- Distance between center button and buttons. -->
        <attr format="dimension" name="distance" />
    </declare-styleable>

    <declare-styleable name="MarqueeViewStyle">
        <attr format="integer|reference" name="mvInterval" />
        <attr format="integer|reference" name="mvAnimDuration" />
        <attr format="dimension|reference" name="mvTextSize" />
        <attr format="color|reference" name="mvTextColor" />
        <attr format="boolean|reference" name="mvSingleLine" />
        <attr format="enum|reference" name="mvGravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr format="enum|reference" name="mvDirection">
            <enum name="bottom_to_top" value="0" />
            <enum name="top_to_bottom" value="1" />
            <enum name="right_to_left" value="2" />
            <enum name="left_to_right" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RingView">
        <attr format="integer|reference" name="totalSection" />
        <attr format="dimension|reference" name="arcBorder" />
        <attr format="dimension|reference" name="arcTextSize" />
        <attr format="color|reference" name="arcSelectTextColor" />
        <attr format="color|reference" name="arcTextColor" />
        <attr format="color|reference" name="arc_bg_color" />
        <attr format="color|reference" name="arc_select_color" />
    </declare-styleable>

    <declare-styleable name="DashboardView">
        <attr format="integer|reference" name="totalAngle" />
        <attr format="dimension|reference" name="centerCircleRadius" />
        <attr format="dimension|reference" name="arcBorderDimen" />
        <attr format="dimension|reference" name="numberTextSize" />
        <attr format="dimension|reference" name="graduatedTextSize" />
        <attr format="dimension|reference" name="angleTextSize" />
